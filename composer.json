    "wordpress-plugin"
  ],
  "homepage": "https://bc.libraries.coop/",
  "authors": [
    {
      "name": "Eric Stainsby",
      "email": "eric.stainsby@roaringsky.ca"
    },
    {
      "name": "Jonathan Schatz",
      "email": "jonathan.schatz@bc.libraries.coop"
    },
    {
      "name": "Ben Holt",
      "email": "ben.holt@bc.libraries.coop"
    },
    {
      "name": "Sam Edwards",
      "email": "sam.edwards@bc.libraries.coop"
    }
  ],
  "require": {
    "php": ">=7.0",
    "composer/installers": "^1.0 || ^2.0",
    "stoutlogic/acf-builder": "^1.12"
  },
  "autoload": {
    "psr-4": {
      "BCLibCoop\\SiteManager\\": "inc/"
    },
    "files": [
      "inc/functions.php"
